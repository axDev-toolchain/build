	  $(GCC_CONFIG_ARGS) --target=$$TARGET --enable-threads --enable-tls --disable-libgomp --disable-libstdc__-v3 --disable-libitm --disable-libatomic) && \
	$(STAMP) $@
build-bootstrap-gcc: stmp-build-bootstrap-gcc
